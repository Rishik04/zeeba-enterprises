{
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://www.zebaenterprises.com/:path*",
      "permanent": true,
      "has": [
        { "type": "host", "value": "zebaenterprises.com" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
